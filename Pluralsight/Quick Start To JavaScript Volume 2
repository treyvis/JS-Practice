//Fizz, buzz, pop interview question

// Print 1 to 100 with #'s divisible by 3 print fizz, divisible by 5 print buzz, divisible by 3 and 5 -> fizzbuzz

for (i = 1; i < 101; i++){
	if (i % 3 === 0 && i % 5 === 0){
		console.log("fizzbuzz");
	} else if (i % 5 === 0){
		console.log("buzz");
	} else if (i % 3 === 0){
		console.log("fizz");
	} else {
		console.log(i);
	}
}